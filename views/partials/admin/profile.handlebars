<div class="feature-video-posts mb-30">
    <!-- Section Title -->
    <div class="section-heading">
        <h5>Thông Tin Cá Nhân</h5>
    </div>
</div>
<!-- >>>>>>>>>>>>>>>>>>>>
             Profile Area
        <<<<<<<<<<<<<<<<<<<<< -->
<form method="post">

    <div class="form-group">
        <label>Họ Tên</label>
        <input type="text" class="form-control" name="" readonly value="{{fullname}}">
    </div>
    <div class="form-group">
        <label>Giới Tính</label>
        <input type="text" class="form-control" name="" readonly value="{{genders}}">
    </div>
    <div class="form-group">
        <label>Ngày Sinh</label>
        <input type="text" class="form-control" name="" readonly value="{{DOB}}">
    </div>
    <div class="form-group">
        <div class="form-group">
            <label>Chứng Minh Nhân Dân</label>
            <input type="text" class="form-control" name="" readonly value="{{cmnd}}">
        </div>
        <label>Số Điện Thoại</label>
        <input type="text" class="form-control" name="phone" value="{{phone}}">
    </div>
    <div class="form-group">
        <label for="exampleFormControlInput1">Email</label>
        <input type="email" class="form-control" name="email" value="{{email}}" placeholder="name@example.com">
    </div>
    <button type="submit" class="btn btn-primary">Cập Nhật</button>


</form>