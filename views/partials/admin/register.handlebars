<div class="feature-video-posts mb-30">
    <!-- Section Title -->
    <div class="section-heading">
        <h5>Thêm Tài Khoản</h5>
    </div>
</div>
<!-- >>>>>>>>>>>>>>>>>>>>
             Profile Area
        <<<<<<<<<<<<<<<<<<<<< -->
<form>
    <div class="form-group">
        <label>Tên Người Dùng</label>
        <input type="text" class="form-control" name="" required="true">
    </div>
    <div class="form-group">
        <label>Tài Khoản</label>
        <input type="text" class="form-control" name="" required="true">
    </div>
    <div class="form-group">
        <label>Mật Khẩu</label>
        <input type="password" class="form-control" name="" required="true">
    </div>
    <div class="form-group">
        <label>Số Điện Thoại</label>
        <input type="Number" class="form-control" name="" required="true">
    </div>
    <div class="form-group">
        <label>Email</label>
        <input type="email" class="form-control" id="Email1" aria-describedby="emailHelp" required="true">
        <small id="emailHelp" class="form-text text-muted">example abc@gamil.com</small>
    </div>
    <div class="form-group">
        <label>Ngày Sinh</label>
        <input type="date" name="bday" required="true">
    </div>
    <div class="form-group">
        <p><label>Giới Tính</label></p>
        <div class="form-check form-check-inline">
            <input class="form-check-input" id="inlineRadio1" type="radio" name="" value="option1">
            <label class="form-check-label" for="inlineRadio1">Nam</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" id="inlineRadio2" type="radio" name="" value="option2">
            <label class="form-check-label" for="inlineRadio2">Nữ</label>
        </div>
    </div>
    <div class="form-group">
        <label>Chứng Minh Nhân Dân</label>
        <input type="Number" class="form-control" name="" required="true">
    </div>
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <label class="input-group-text" for="permissionSelect">Phân Quyền</label>
        </div>
        <select class="custom-select" id="permissionSelect">
            <option selected>Choose...</option>
            <option value="admin">Quản Trị Viên</option>
            <option value="editor">Biên Tập Viên</option>
        </select>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="option1" disabled="true">
        <label class="form-check-label" for="inlineCheckbox1">Thêm</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2" disabled="true">
        <label class="form-check-label" for="inlineCheckbox2">Sửa</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option3" disabled="true">
        <label class="form-check-label" for="inlineCheckbox2">Xóa</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option4" disabled="true">
        <label class="form-check-label" for="inlineCheckbox2">Duyệt Bài</label>
    </div>
    </br>
    </br>
    </br>
    <button type="submit" class="btn btn-primary">Thêm Tài Khoản</button>
</form>